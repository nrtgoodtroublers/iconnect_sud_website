{
  "hosting": {
    "site": "iconnect-landing",
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [
      {
        "source": "/register",
        "destination": "https://app.iconnectcounseling.org/register",
        "type": 301
      },
      {
        "source": "/login",
        "destination": "https://app.iconnectcounseling.org/login",
        "type": 301
      },
      {
        "source": "/logout",
        "destination": "https://app.iconnectcounseling.org/logout",
        "type": 301
      },
      {
        "source": "/password_reset",
        "destination": "https://app.iconnectcounseling.org/password_reset",
        "type": 301
      },
      {
        "source": "/request_activation",
        "destination": "https://app.iconnectcounseling.org/request_activation",
        "type": 301
      },
      {
        "source": "/activate/**",
        "destination": "https://app.iconnectcounseling.org/activate",
        "type": 301
      },
      {
        "source": "/reset/**",
        "destination": "https://app.iconnectcounseling.org/reset",
        "type": 301
      },
      {
        "source": "/admin/**",
        "destination": "https://app.iconnectcounseling.org/admin",
        "type": 301
      },
      {
        "source": "/accounts/**",
        "destination": "https://app.iconnectcounseling.org/accounts",
        "type": 301
      },
      {
        "source": "/record/:slug",
        "destination": "https://app.iconnectcounseling.org/record/:slug",
        "type": 301
      },
      {
        "source": "/add_resource",
        "destination": "https://app.iconnectcounseling.org/add_resource",
        "type": 301
      },
      {
        "source": "/update_resource/:slug",
        "destination": "https://app.iconnectcounseling.org/update_resource/:slug",
        "type": 301
      },
      {
        "source": "/delete_record/:slug",
        "destination": "https://app.iconnectcounseling.org/delete_record/:slug",
        "type": 301
      },
      {
        "source": "/disable_record/:slug",
        "destination": "https://app.iconnectcounseling.org/disable_record/:slug",
        "type": 301
      },
      {
        "source": "/upload_csv",
        "destination": "https://app.iconnectcounseling.org/upload_csv",
        "type": 301
      },
      {
        "source": "/profile/:username",
        "destination": "https://app.iconnectcounseling.org/profile/:username",
        "type": 301
      },
      {
        "source": "/profile/:username/my_resources",
        "destination": "https://app.iconnectcounseling.org/profile/:username/my_resources",
        "type": 301
      },
      {
        "source": "/profile/:username/my_resources/:slug",
        "destination": "https://app.iconnectcounseling.org/profile/:username/my_resources/:slug",
        "type": 301
      },
      {
        "source": "/profile/:username/:slug",
        "destination": "https://app.iconnectcounseling.org/profile/:username/:slug",
        "type": 301
      },
      {
        "source": "/api/**",
        "destination": "https://app.iconnectcounseling.org/api",
        "type": 301
      }
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}